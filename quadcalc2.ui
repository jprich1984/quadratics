<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="ttk.Frame" id="top_level" named="True">
    <property name="height">200</property>
    <property name="padding">10</property>
    <property name="width">200</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Label" id="header_label" named="True">
        <property name="font">{Arial} 24 {}</property>
        <property name="justify">left</property>
        <property name="padding">10</property>
        <property name="relief">raised</property>
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">Quadratic Function Roots Calculator</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label2">
        <property name="image">quadfun.png</property>
        <property name="text" translatable="yes">label2</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="description_1" named="True">
        <property name="font">{Arial} 20 {}</property>
        <property name="padding">10</property>
        <property name="text" translatable="yes">Enter values for a, b, and c.</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">2</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="input_frame" named="True">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">3</property>
          <property name="sticky">w</property>
        </layout>
        <child>
          <object class="ttk.Label" id="a_label" named="True">
            <property name="font">{Arial} 20 {}</property>
            <property name="text" translatable="yes">a=</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="a_entry" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="b_label" named="True">
            <property name="font">{Arial} 20 {}</property>
            <property name="text" translatable="yes">b=</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="b_entry" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="c_label" named="True">
            <property name="font">{Arial} 20 {}</property>
            <property name="text" translatable="yes">c=</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="c_entry" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="button_frame" named="True">
        <property name="height">200</property>
        <property name="width">350</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">4</property>
          <property name="sticky">w</property>
        </layout>
        <child>
          <object class="ttk.Button" id="ok_button" named="True">
            <property name="command" type="command" cbtype="simple">calculate</property>
            <property name="text" translatable="yes">OK</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="clear_button" named="True">
            <property name="command" type="command" cbtype="simple">clear</property>
            <property name="text" translatable="yes">Clear</property>
            <layout manager="grid">
              <property name="column">3</property>
              <property name="padx">30</property>
              <property name="pady">30</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
